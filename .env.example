# ================== BYBIT API ==================
# Получить на https://www.bybit.com/app/user/api-management
BYBIT_API_KEY=your_api_key_here
BYBIT_SECRET=your_secret_here
BYBIT_TESTNET=false

# ================== TELEGRAM ==================
# Создать бота через @BotFather
TELEGRAM_TOKEN=your_telegram_token_here
# Узнать chat_id через @userinfobot
TELEGRAM_CHAT_ID=your_chat_id_here

# ================== DATABASE ==================
# Для Docker - не менять
DATABASE_URL=postgresql://bot:botpassword@postgres:5432/breakout_bot

# Для локальной разработки (без Docker)
# DATABASE_URL=postgresql://bot:botpassword@localhost:5432/breakout_bot

# ================== LOGGING ==================
LOG_LEVEL=INFO
